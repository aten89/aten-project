<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="org.eapp.crm.hbean.ReportNormConf" table="CRM_RPT_NORM_CONF">
		
		<id name="id" type="java.lang.String">
			<column name="ID_" length="36" />
			<generator class="uuid.hex"></generator>
		</id>
        <property name="rptID" type="java.lang.String">
            <column name="RPT_ID_" length="36" />
        </property>
        <property name="normCode" type="java.lang.String">
            <column name="NORM_CODE_" length="50" />
        </property>
        <property name="normName" type="java.lang.String">
            <column name="NORM_NAME_" length="100" />
        </property>
        <property name="normValue" type="java.lang.String">
            <column name="NORM_VALUE_" length="200" />
        </property>
	</class>
</hibernate-mapping>


