<?xml version='1.0' encoding='UTF-8'?>
<wfm-configuration>
	<!-- hibernate 配置文件 -->
	<string name="resource.hibernate.cfg.xml" value="/wfm.hibernate.cfg.xml" />
	<!-- 系统属性 配置文件 -->
	<string name="resource.system.properties" value="/wfm-config.properties" />

	<!-- 结点类型 -->
	<map name="node.types">
	  <element key="start-node" value="org.eapp.workflow.def.node.StartNode" />
	  <element key="end-node" value="org.eapp.workflow.def.node.EndNode" />
	  <element key="node" value="org.eapp.workflow.def.Node" />
	  <element key="task-node" value="org.eapp.workflow.def.node.TaskNode" />
	  <element key="decision" value="org.eapp.workflow.def.node.DecisionNode" />
	  <element key="fork" value="org.eapp.workflow.def.node.ForkNode" />
	  <element key="join" value="org.eapp.workflow.def.node.JoinNode" />
	  <element key="subflow-node" value="org.eapp.workflow.def.node.SubFlowNode" />
	</map>
	
	<!-- 动作类型 -->
	<map name="action.types">
	  <element key="action" value="org.eapp.workflow.def.Action" />
	</map>
</wfm-configuration>
