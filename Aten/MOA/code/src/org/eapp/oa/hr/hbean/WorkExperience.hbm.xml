<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="org.eapp.oa.hr.hbean.WorkExperience" table="OA_WORKEXPERIENCE">
		
		<id name="id" type="java.lang.String">
			<column name="ID_" length="36" />
			<generator class="uuid.hex"></generator>
		</id>
		<property name="startDate" type="java.util.Date">
			<column name="STARTDATE_" />
		</property>
		<property name="endDate" type="java.util.Date">
			<column name="ENDDATE_" />
		</property>
		<property name="companyName" type="java.lang.String">
			<column name="COMPANYNAME_" length="100" />
		</property>
        <property name="postName" type="java.lang.String">
            <column name="POSTNAME_" length="100" />
        </property>
        <property name="postDuty" type="java.lang.String">
            <column name="POSTDUTY_" length="100" />
        </property>
	</class>
</hibernate-mapping>


